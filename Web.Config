<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<RewriterConfig>
		<Rules>
			<RewriterRule>
				<match url="^(application|modules|system|tests|sql)"  />
				<action type="Rewrite" url="/-"  />
			</RewriterRule>
			<RewriterRule>
				<match url=".*"  />
				<action type="Rewrite" url="/index.php?kohana_uri={R:0}"  appendQueryString="true" />
			</RewriterRule>
		</Rules>
	 </RewriterConfig>
</configuration>