msgctxt "report.custom_field.values"
msgid "Please enter a valid value for the \"%s\" field."
msgstr "សូម​បញ្ចូល​តម្លៃ​ត្រឹមត្រូវ​សម្រាប់​វាល \"%s\" ។"

