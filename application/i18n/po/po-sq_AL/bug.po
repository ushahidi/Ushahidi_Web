#: bug.captcha.default
msgctxt "bug.captcha.default"
msgid "Please enter a valid security code."
msgstr "Ju lutem shkruani nj&euml; kod sigurie t&euml; vlefsh&euml;m."

#: bug.captcha.required
msgctxt "bug.captcha.required"
msgid "Please enter the security code."
msgstr "Ju lutem vendosni kodin e siguris&euml;."

#: bug.email.email
msgctxt "bug.email.email"
msgid "The Email field does not appear to contain a valid email address?"
msgstr "Adresa e email-it e vendosur n&euml; hapsir&euml;n e m&euml;poshtme nuk &euml;sht&euml; e vlefshme"

#: bug.email.length
msgctxt "bug.email.length"
msgid "The Email field must be at least 4 and no more 64 characters long."
msgstr "Adresa e email-it duhet t&euml; p&euml;rmbaj&euml; 4 deri n&euml; 64 karaktere."

#: bug.email.required
msgctxt "bug.email.required"
msgid "The Email field is required if the checkbox is checked."
msgstr "Informacioni p&euml;r adres&euml;n e email i detyruesh&euml;m p&euml;r tu plot&euml;suar."

#: bug.error.required
msgctxt "bug.error.required"
msgid "The error field is required."
msgstr "Informacioni i detyruesh&euml;m p&euml;r tu plot&euml;suar ."

#: bug.subject.length
msgctxt "bug.subject.length"
msgid "The subject field must be at least 3 characters long."
msgstr "Subjekti duhet t&euml; p&euml;rmbaj&euml; t&euml; pakt&euml;n 3 karaktere."

#: bug.subject.required
msgctxt "bug.subject.required"
msgid "The subject field is required."
msgstr "Informacioni p&euml;r subjektin i detyruesh&euml;m p&euml;r tu plot&euml;suar."

#: bug.yourname.length
msgctxt "bug.yourname.length"
msgid "The name field must be at least 3 characters long."
msgstr "Emri duhet t&euml; p&euml;rmbaj&euml; t&euml; pakt&euml;n 3 karaktere."

#: bug.yourname.required
msgctxt "bug.yourname.required"
msgid "The name field is required."
msgstr "Informacioni p&euml;r emrin i detyruesh&euml;m p&euml;r tu plot&euml;suar."
